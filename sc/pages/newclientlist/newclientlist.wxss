/* pages/newclientlist/newclientlist.wxss */
@import "../aaafirst/aaafirst.wxss";

.client_category_class{
  width: 750rpx;
  display: flex;
  height: 80rpx;
  background: white;
  border-bottom: 1rpx solid #eeeeee;
  flex-wrap: nowrap;
}
.client_category_class>text{
  height: 80rpx;
  /* width: 250rpx; */
  line-height: 80rpx;
  font-size: 28rpx;
  text-align: center;
  display: block;
  flex: 1;
}
.row_style{
  display: flex;
  height: 188rpx;
  width: 750rpx;
  background: white;
  border-bottom: 1rpx solid #eeeeee;
}
.row_img{
  margin-left: 30rpx;
  margin-top: 30rpx;
  height: 128rpx;
  width: 128rpx;
  background: white;
}
.right_row_class{
  margin-top: 30rpx;
  height: 128rpx;
  width: 562rpx;
  margin-left: 30rpx;
}
.right_up_row_class{
  height: 40rpx;
  width: 562rpx;
  display: flex;
}
.right_text_up_row_class{
  display: block;
  height: 60rpx;
  line-height: 60rpx;
  text-align: left;
  font-size: 32rpx;
  color: #333333;
  width: 400rpx;
}
.right_right_text_up_row_class{
  display: block;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
  font-size: 24rpx;
  color: #999999;
  width: 130rpx;
}
.address_style{
  display: block;
  margin-top: 20rpx;
  text-align: left;
  font-size: 24rpx;
  color: #999999;
  line-height: 30rpx;
}